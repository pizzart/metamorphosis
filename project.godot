config_version=5

[application]

config/name="Metamorphosis"
run/main_scene="res://scenes/menu.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"
run/main_scene.editor="res://scenes/world.tscn"

[audio]

driver/output_latency=40

[autoload]

UI="*res://scenes/ui.tscn"
FX="*res://scenes/fx.tscn"
PauseMenu="*res://scenes/pause_menu.tscn"
Global="*res://scripts/global.gd"
CyclopsAutoload="*res://addons/cyclops_level_builder/cyclops_global_scene.tscn"

[display]

window/size/viewport_width=320
window/size/viewport_height=240
window/size/window_width_override=960
window/size/window_height_override=720
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/per_pixel_transparency/allowed=true
mouse_cursor/custom_image="res://sprites/cursor_menu.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/cyclops_level_builder/plugin.cfg")

[gui]

